{
  "name": "qin-admin-monorepo",
  "version": "1.0.0",
  "description": "ðŸš€ Qin Admin is a production-ready front-end solution for admin interfaces. Based on Vue 3, TypeScript, Vite, and Arco Design.",
  "keywords": [
    "vue",
    "typescript",
    "vue3",
    "ts",
    "tsx",
    "admin",
    "arco-design",
    "arco-design-pro-vue",
    "arco-design-pro"
  ],
  "homepage": "https://github.com/LLiuHuan/qin-admin",
  "bugs": {
    "name": "LLiuHuan",
    "email": "liuhuan@0x3.cn",
    "url": "https://github.com/lliuhuan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LLiuHuan/qin-admin.git"
  },
  "license": "MIT",
  "author": {
    "name": "LLiuHuan",
    "email": "liuhuan@0x3.cn",
    "url": "https://github.com/lliuhuan"
  },
  "type": "module",
  "website": "https://lliuhuan.github.io/qin-admin",
  "documentation": "https://github.com/LLiuHuan/qin-admin",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:analyze": "turbo build:analyze",
    "build:antd": "pnpm run build --filter=@qin/web-antd",
    "build:arco": "pnpm run build --filter=@qin/web-arco",
    "build:docker": "./scripts/deploy/build-local-docker-image.sh",
    "build:ele": "pnpm run build --filter=@qin/web-ele",
    "build:naive": "pnpm run build --filter=@qin/web-naive",
    "build:tdesign": "pnpm run build --filter=@qin/web-tdesign",
    "build:play": "pnpm run build --filter=@qin/playground",
    "changeset": "pnpm exec changeset",
    "check": "pnpm run check:circular && pnpm run check:dep && pnpm run check:type && pnpm check:cspell",
    "check:circular": "vsh check-circular",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "check:dep": "vsh check-dep",
    "check:type": "turbo run typecheck",
    "clean": "node ./scripts/common/clean.mjs",
    "commit": "czg",
    "dev": "turbo-run dev",
    "dev:antd": "pnpm -F @qin/web-antd run dev",
    "dev:docs": "pnpm -F @qin/docs run dev",
    "dev:ele": "pnpm -F @qin/web-ele run dev",
    "dev:naive": "pnpm -F @qin/web-naive run dev",
    "dev:arco": "pnpm -F @qin/web-arco run dev",
    "dev:tdesign": "pnpm -F @qin/web-tdesign run dev",
    "dev:play": "pnpm -F @qin/playground run dev",
    "format": "vsh lint --format",
    "lint": "vsh lint",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky",
    "preview": "turbo-run preview",
    "publint": "vsh publint",
    "reinstall": "pnpm clean --del-lock && pnpm install",
    "test:unit": "vitest run --dom",
    "test:e2e": "turbo run test:e2e",
    "update:deps": "npx taze -r -w",
    "version": "pnpm exec changeset version && pnpm install --no-frozen-lockfile"
  },
  "devDependencies": {
    "@changesets/changelog-github": "catalog:",
    "@changesets/cli": "catalog:",
    "@playwright/test": "catalog:",
    "@qin/commitlint-config": "workspace:*",
    "@qin/eslint-config": "workspace:*",
    "@qin/prettier-config": "workspace:*",
    "@qin/stylelint-config": "workspace:*",
    "@qin/tsconfig": "workspace:*",
    "@qin/turbo-run": "workspace:*",
    "@qin/uno-config": "workspace:*",
    "@qin/vite-config": "workspace:*",
    "@qin/vsh": "workspace:*",
    "@types/node": "catalog:",
    "@unocss/postcss": "catalog:",
    "@unocss/preset-wind4": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vue/test-utils": "catalog:",
    "autoprefixer": "catalog:",
    "cross-env": "catalog:",
    "cspell": "catalog:",
    "happy-dom": "catalog:",
    "husky": "catalog:",
    "is-ci": "catalog:",
    "lint-staged": "catalog:",
    "playwright": "catalog:",
    "rimraf": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unocss": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {
      "@ast-grep/napi": "catalog:",
      "@ctrl/tinycolor": "catalog:",
      "clsx": "catalog:",
      "esbuild": "0.25.3",
      "pinia": "catalog:",
      "vue": "catalog:"
    },
    "neverBuiltDependencies": [
      "canvas",
      "node-gyp"
    ]
  }
}
